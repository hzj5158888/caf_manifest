+
+#include <private/bionic_asm.h>
+
+ENTRY(lround)
+  fcvtas x0, d0
+  ret
+END(lround)
+
+ENTRY(lroundf)
+  fcvtas x0, s0
+  ret
+END(lroundf)
+
+// sizeof(long) and sizeof(long long) are the same for aarch64
+ALIAS_SYMBOL(llround, lround);
+
+ALIAS_SYMBOL(llroundf, lroundf);
